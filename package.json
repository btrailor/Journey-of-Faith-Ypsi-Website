{
  "name": "church-website",
  "version": "1.0.0",
  "description": "Static church website built with 11ty, Bootstrap, and Decap CMS",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:eleventy": "eleventy --serve",
    "start:sass": "sass --watch src/sass:_site/css",
    "build": "npm-run-all build:*",
    "build:eleventy": "eleventy",
    "build:sass": "sass src/sass:_site/css --style=compressed"
  },
  "keywords": ["church", "website", "11ty", "bootstrap"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.2"
  }
}
